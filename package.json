{
  "name": "react-template-ts-mobx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "dev": "concurrently \"vite --host --mode development\" \"yarn typed:css -w\" \"yarn typed:scss --watch\"",
    "typed:css": "tcm -p \"src/**/*.module.css\"",
    "typed:scss": "typed-scss-modules \"src/**/*.module.scss\"",
    "build": "yarn typed:css && yarn typed:scss && tsc && vite build --mode production --config vite.config.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "ismobilejs": "^1.1.1",
    "js-cookie": "^3.0.5",
    "mobx": "^6.12.4",
    "mobx-persist-store": "^1.1.5",
    "mobx-react": "^9.1.1",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.24.0",
    "sass": "^1.77.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@stylistic/eslint-plugin": "^2.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "app-root-path": "^3.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^26.1.0",
    "papaparse": "^5.4.1",
    "prettier": "3.5.3",
    "typed-css-modules": "^0.9.1",
    "typed-scss-modules": "^8.0.1",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "vite": "^5.3.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^3.1.2"
  },
  "packageManager": "yarn@4.3.1"
}
